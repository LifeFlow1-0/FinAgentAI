# Gamma (Staging) Environment Variables

# Database
DATABASE_URL=postgresql://user:password@db:5432/gamma_db

# Plaid API
PLAID_CLIENT_ID=your_gamma_client_id
PLAID_SECRET=your_gamma_secret
PLAID_ENV=development
PLAID_REDIRECT_URI=https://gamma.lifeflow.app/oauth-callback

# Security
ENCRYPTION_KEY=gamma_encryption_key_example
DEBUG=false

# Monitoring
SENTRY_DSN=your_sentry_dsn
NEW_RELIC_LICENSE_KEY=your_newrelic_key
PROMETHEUS_MULTIPROC_DIR=/tmp/prometheus-metrics

# Logging
LOG_LEVEL=INFO 
# Production Environment Variables

# Database
DATABASE_URL=postgresql://user:password@db:5432/prod_db

# Plaid API
PLAID_CLIENT_ID=your_prod_client_id
PLAID_SECRET=your_prod_secret
PLAID_ENV=production
PLAID_REDIRECT_URI=https://lifeflow.app/oauth-callback

# Security
ENCRYPTION_KEY=prod_encryption_key_example
DEBUG=false

# Monitoring
SENTRY_DSN=your_sentry_dsn
PROMETHEUS_MULTIPROC_DIR=/tmp/prometheus-metrics

# Logging
LOG_LEVEL=WARNING 